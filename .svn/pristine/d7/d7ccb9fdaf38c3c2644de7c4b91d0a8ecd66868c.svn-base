}

.m-popbrands .recomBrand .itm {
    position: relative;
    float: left;
    width: 219px;
    height: 176px;
    border-right: 1px solid #e9e9e9;
    border-bottom: 1px solid #e9e9e9;
    overflow: hidden;
}

.m-popbrands .recomBrand .info {
    margin: 0 auto;
    /* width: 0; */
    height: 100%;
    overflow: hidden;
    background: #fff;
    /* display:none; */
    -webkit-transform: translateZ(0);
    -moz-transform: translateZ(0);
    -ms-transform: translateZ(0);
    -o-transform: translateZ(0);
    transform: translateZ(0);

}

.m-popbrands .recomBrand .itm .info img {
    position: relative;
    top: 0;
    -ms-transition: top .3s ease, transform .3s ease;
    transition: top .3s ease, transform .3s ease;
    -ms-transform: scale(1);
    transform: scale(1);
}

.m-popbrands .recomBrand .img {
    display: block;
    margin: 12px auto 0;
    width: 120px;
    height: 120px;
}

.m-popbrands .recomBrand .itm .info .desc {
    -ms-transition: opacity .2s ease .08s;
    transition: opacity .2s ease .08s;
}

.m-popbrands .recomBrand .desc {
    margin: 10px -90px 0;
    text-align: center;
    white-space: nowrap;
    overflow: hidden;
    font-size: 14px;
    font-weight: 700;
}

.m-popbrands .recomBrand .actions,
.m-popbrands .recomBrand .mask {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    opacity: 0;
    filter: alpha(opacity=0);
    text-align: center;
    -webkit-transition: all .3s ease;
    -moz-transition: all .3s ease;
    -ms-transition: all .3s ease;
    -o-transition: all .3s ease;
    transition: all .3s ease;
}

.m-popbrands .recomBrand .follow {
    display: inline-block;
    position: absolute;
    top: 10px;
    left: 10px;
    width: 52px;
    height: 20px;
    line-height: 20px;
    color: #999;
    border: 1px solid #999;
    text-decoration: none;
    opacity: 0;
    filter: alpha(opacity=0);
    -webkit-border-radius: 20px;
    -moz-border-radius: 20px;
    border-radius: 20px;
}

.m-popbrands .recomBrand .followers {
    margin: 99px 0 8px;
    color: #999;
}

.m-popbrands .recomBrand .enter {
    display: block;
    margin: 0 auto;
    width: 80px;
    height: 36px;
    box-sizing: border-box;
    line-height: 32px;
    color: #ff1e3c;
