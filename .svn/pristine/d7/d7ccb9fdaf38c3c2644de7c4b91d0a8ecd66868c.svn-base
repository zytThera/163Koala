.container {
    width: 1100px;
    margin: 0 auto;

}

/* #header{
    overflow: hidden;
} */
/* 顶部广告区 */
#top_title_line {
    background: #D22147;
}

#top_ads {
    height: 80px;
    width: 1920px;
    margin: 0 auto;
    overflow: hidden;
}

#top_ads a {
    display: block;
    width: 1920px;
    height: 80px;
}



/* 头部导航 */
#top_nav {
    position: relative;
    width: 100%;
    height: 30px;
    line-height: 30px;
    background: #000;
}


#top_navleft {
    float: left;
}

#top_navleft .login {
    margin-left: 16px;
    margin-right: 10px;
}

#top_navleft>a,
#top_navleft>span {
    color: #999;
}

#top_navmiddle>a {
    color: #999;
}

#top_navmiddle {
    float: left;
    padding-left: 30px;
}

#top_navright {
    float: right;
}

#top_navright>li {
    float: left;
    padding: 0 14px 0 8px;
}

#top_navright>li>a {
    color: #999;
}

/* 头部搜索区  */
#top_search {
    width: 100%;
    height: 100px;
    /* padding-top: 30px; */
    box-sizing: border-box;
    padding-top: 18px;
}

#top_search .s-inner {
    box-sizing: border-box;
    height: 100px;
    position: relative;
}

#top_search .topTabBoxFixed {
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    height: 50px;
    z-index: 1001;
    background: #fff;
    border-bottom: 1px solid #ddd;
    box-shadow: 0 0 10px rgba(0, 0, 0, .2);
}

/* logo */
#logo {
    width: 330px;
    height: 65px;
    float: left;
}

#top_search .logokaola {
    display: block;
    width: 330px;
    height: 64px;
    line-height: 64px;
    cursor: pointer;
    background: url(../images/logo.png) no-repeat;
    background-size: contain;
}

/* 搜索框 */
#top_search{
    background: #fff;
}
#topsearchbox {
    float: left;
    position: relative;
}

#topsearch {
    width: 510px;
    height: 40px;
    background-color: #ff2337;
    border-radius: 40px;
}

#topsearch .wrap {
    float: left;
    margin: 2px 1px;
    padding: 6px 39px 6px 30px;
    background-color: #fff;
    border-radius: 24px 0 0 24px;
}

#searchinput {
    width: 410px;
    height: 24px;
    line-height: 24px;
    border: 0;
    font-size: 13px;
}

.topsearchboxBackGround {
    width: 60px;
    height: 40px;
    background: #ff2337;
    background-image: linear-gradient(270deg, #ff2e64, #ff0003);
    border-radius: 40px;
    position: absolute;
    top: 0;
    right: 0;
}

#topSearchBtn {
    display: inline-block;
    height: 40px;
    width: 50px;
    font-size: 19px;
    line-height: 40px;
    color: #fff;
    text-align: center;
    cursor: pointer;
    position: absolute;
    top: 0;
    right: 5px;
}

.icos {
    position: absolute;
    left: 10px;
    top: 13px;
    height: 14px;
    width: 14px;
}

#search_ul li:first-child{
    margin-top:15px;
}

#search_ul li{
    height:30px;
    line-height: 30px;
    color:#666;
}
#search_ul li:hover a {
   color:	#ff1e32;;
}

/* 搜索框下面的推荐单 */
#suggestlist {
    line-height: 22px;
    margin-top: 2px;
    width: 461px;
    height: 22px;
    overflow: hidden;
}

#suggestlist li {
    float: left;
    white-space: nowrap;
    margin-left: 30px;
}

/* 购物车 */
#cart {
    float: right;
    margin-right: 70px;
}

.m-shopcartnew {
    display: block;
    /* position: absolute;
    right: 356px;
    top: 33px; */
    width: 106px;
    height: 36px;
    background-color: #fff;
    line-height: 36px;
    font-size: 14px;
    text-align: center;
    -moz-border-radius: 36px;
    -webkit-border-radius: 36px;
    border-radius: 36px;
    border: 2px solid #ff1e32;
}

.m-shopcartnew>.icon-cart {
    color: #ff1e32;
}

.num {
    display: inline-block;
    *display: inline;
    *zoom: 1;
    width: 20px;
    height: 20px;
    border-radius: 50%;
    text-align: center;
    line-height: 20px;
    font-size: 12px;
    font-weight: 400;
    color: #fff;
    background: #ff1e32;
    white-space: nowrap;
    font-family: Helvetica, arial;
    margin-left: 2px;
    margin-top: -2px;
}

/* 购物车结束 */



/* 功能选项卡区 */
#topTab {
    height: 40px;
    border-bottom: 1px solid #ccc;
    background: #fff;
    z-index: 102;
}

#topTab .tab-inner {
    height: 40px;
    position: relative;
}

.tab-inner .shell {
    overflow: hidden;
}

.navitm-cats {
    height: 40px;
    font-size: 14px;
    text-align: center;
    padding-left: 0;
    overflow: hidden;
}


.navitm-cats>.toplevel {
    background: red;
    color: #fff;
    display: block;
    float: left;
    height: 40px;
    line-height: 40px;
    width: 164px;
    text-align: left;
    border-bottom: 1px solid rgba(238, 230, 230, 0.4);
}

.toplevel .lineicon {
    display: inline-block;
    margin: 0 24px 0 15px;
}

.navitm-cats #funcTab {
    float: left;
    padding-left: 25px;
    height: 40px;

}

.navitm-cats #funcTab li {
    float: left;
    height: 40px;
    padding: 0 20px;
    line-height: 40px;
}

.toplevel:link,
.toplevel:visited {
    background: #fff;
    color: #ff1e32;
}

.catitmlst {
    position: absolute;
    width: 164px;
    float: left;
    top: 40px;
    z-index: 99;
    background: #f00;
    background: -webkit-linear-gradient(to right, #f00, #ff3264);
    background: linear-gradient(to right, #f00, #ff3264);
}

.catitmlst li {
    color: #fff;
    background: red;
    background: -webkit-linear-gradient(90deg, red, #ff3264);
    background: linear-gradient(90deg, red, #ff3264);
    padding-left: 10px;
    width: 151px;
    cursor: pointer;
    text-align: left;
    padding-right: 3px;
    height: 42px;
    line-height: 42px;
    border-bottom: 1px solid rgba(238, 230, 230, 0.4);
}

.catitmlst .imgicon {
    width: 20px;
    height: 20px;
    vertical-align: middle;
    padding: 0 20px 0 4px;
    color: white;
    display: inline-block;
}




